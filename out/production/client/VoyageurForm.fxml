<AnchorPane prefHeight="331.0" prefWidth="440.0" style="-fx-background-color: #FCC6AB;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bda1.control.VoyageurFromController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Voyageur">
            <font>
                <Font name="SansSerif Bold" size="22.0" />
            </font>
        </Label>
        <HBox alignment="CENTER_LEFT" layoutX="43.0" layoutY="49.0" prefHeight="41.0" prefWidth="383.0">
            <children>
                <Label prefHeight="15.0" prefWidth="180.0" text="Nom" />
                <JFXTextField fx:id="nom" prefHeight="25.0" prefWidth="199.0" />
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="43.0" layoutY="115.0" prefHeight="41.0" prefWidth="383.0">
            <children>
                <Label prefHeight="15.0" prefWidth="180.0" text="Prenom" />
                <JFXTextField fx:id="prenom" prefHeight="25.0" prefWidth="199.0" />
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="43.0" layoutY="175.0" prefHeight="41.0" prefWidth="383.0">
            <children>
                <Label prefHeight="15.0" prefWidth="180.0" text="Date de Nissance" />
            <DatePicker id="dateNissance" />
            </children>
        </HBox>

        <HBox alignment="CENTER_RIGHT" layoutX="220.0" layoutY="284.0" prefHeight="41.0" prefWidth="200.0">
            <children>
                <Button fx:id="closeBtn" mnemonicParsing="false" onAction="#onClose" style="-fx-background-color: #AF4B57;-fx-text-fill: #fff;" text="Close">
                    <effect>
                        <DropShadow />
                    </effect>
                    <HBox.margin>
                        <Insets right="20.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="saveBtn" mnemonicParsing="false" style="-fx-background-color: #AF4B57;-fx-text-fill: #fff;" text="Sauvgarder">
                    <effect>
                        <DropShadow />
                    </effect>
                    <HBox.margin>
                        <Insets right="20.0" />
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </children>
</AnchorPane>
